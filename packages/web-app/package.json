  "scripts": {
    "start:prod": "npm run build:prod && npm run preview",
    "start:staging": "npm run build:prod && npm run preview",
    "start:dev": "npm run build:dev && npm run preview",
    "build:prod": "vue-tsc --noEmit && vite build --emptyOutDir",
    "build:staging": "vue-tsc --noEmit && vite build --emptyOutDir --mode staging",
    "build:dev": "vue-tsc --noEmit && vite build --emptyOutDir --mode development",
    "dev": "vite",
    "dev:prod": "vite --mode production",
    "preview": "vite preview --host",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix src"
  },
  "dependencies": {
    "@ethersproject/providers": "^5.7.2",
    "@metamask/onboarding": "^1.0.1",
    "@metamask/providers": "^10.2.0",
    "ethers": "^5.7.2",
    "jsbn": "^1.1.0",
    "naive-ui": "^2.31.0",
    "pinia": "^2.0.11",
    "secure-random": "^1.1.2",
    "vue": "^3.2.37",
    "vue-query": "^2.0.0-beta.6",
    "vue-router": "^4.1.1"
  },
  "devDependencies": {
    "@types/jsbn": "^1.2.30",
    "@types/node": "^17.0.18",
    "@types/secure-random": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5.12.0",
    "@typescript-eslint/parser": "^5.12.0",
    "@vitejs/plugin-vue": "^3.0.1",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-vue": "^8.4.1",
    "postcss": "^8.4.7",
    "postcss-scss": "^4.0.3",
    "sass": "^1.49.8",
    "stylelint": "^14.6.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "typescript": "^4.6.4",
    "vite": "3.0.9",
    "vite-plugin-mkcert": "^1.9.0",
